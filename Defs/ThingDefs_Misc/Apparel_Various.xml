    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
		<li>Neck</li>
        <li>Shoulders</li>
		<li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/GermUni/GermU</wornGraphicPath>
      <layers>
        <li>Middle</li>
      </layers>
      <tags>
        <li>IndustrialMilitaryBasic</li>
        <li>IndustrialAdvanced</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
