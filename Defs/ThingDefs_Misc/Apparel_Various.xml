<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef Name="ApparelBase" Abstract="True">
    <thingClass>Apparel</thingClass>
    <category>Item</category>
    <selectable>True</selectable>
    <pathCost>10</pathCost>
    <useHitPoints>True</useHitPoints>
    <techLevel>Medieval</techLevel>
    <graphicData>
      <onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
    </graphicData>
    <drawGUIOverlay>true</drawGUIOverlay>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>2</DeteriorationRate>
      <Beauty>-4</Beauty>
      <SellPriceFactor>0.70</SellPriceFactor>
    </statBases>
    <altitudeLayer>Item</altitudeLayer>
    <alwaysHaulable>True</alwaysHaulable>
    <tickerType>Never</tickerType>
    <comps>
      <li Class="CompProperties_Forbiddable"/>
      <li>
        <compClass>CompColorable</compClass>
      </li>
      <li>
        <compClass>CompQuality</compClass>
      </li>
    </comps>
  </ThingDef>
  <ThingDef Name="ApparelMakeableBase" ParentName="ApparelBase" Abstract="True">
    <recipeMaker>
      <workSpeedStat>TailoringSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Tailor</effectWorking>
      <soundWorking>Recipe_Tailor</soundWorking>
      <recipeUsers>
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
      </recipeUsers>
      <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
    </recipeMaker>
  </ThingDef>
  <!-- =================================== Light Uniform ====================================-->
  <ThingDef ParentName="ApparelMakeableBase">
    <defName>Apparel_GermUniL</defName>
    <label>Light Uniform</label>
    <description>Light Army Uniform that provides little protection against attacks, Made to be warm instad of protective</description>
	<techLevel>Industrial</techLevel>
    <graphicData>
      <texPath>Things/Apparel/GermUni/Germ</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>120</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
    </stuffCategories>
	<recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
	  <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
    </recipeMaker>
    <generateCommonality>1.25</generateCommonality>
    <statBases>
      <WorkToMake>5500</WorkToMake>
      <MaxHitPoints>150</MaxHitPoints>
	  <ArmorRating_Blunt>4</ArmorRating_Blunt>
	  <ArmorRating_Heat>0.4</ArmorRating_Heat>
	  <Insulation_Cold>25</Insulation_Cold>
	  <StuffEffectMultiplierInsulation_Cold>0.85</StuffEffectMultiplierInsulation_Cold>
	  <StuffEffectMultiplierInsulation_Heat>0.65</StuffEffectMultiplierInsulation_Heat>
	  <StuffEffectMultiplierArmor>1</StuffEffectMultiplierArmor>
      <Mass>2.0</Mass>
      <Bulk>3.5</Bulk>
	  <EquipDelay>4</EquipDelay>
    </statBases>
	<equippedStatOffsets>
      <CarryBulk>10</CarryBulk>
	  <MoveSpeed>0.2</MoveSpeed>
    </equippedStatOffsets>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
		<li>Neck</li>
        <li>Shoulders</li>
		<li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/GermUni/GermU</wornGraphicPath>
      <layers>
        <li>Middle</li>
      </layers>
      <tags>
	    <li>Soldier</li>
        <li>IndustrialMilitaryBasic</li>
        <li>IndustrialAdvanced</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
  <!-- =================================== German Uniform ====================================-->
  <ThingDef ParentName="ApparelMakeableBase">
    <defName>Apparel_GermUni</defName>
    <label>Army Uniform</label>
    <description>Standard Army Uniform that provides limited protection against attacks.</description>
    <recipeMaker>
      <researchPrerequisite>FullBodyUniforms</researchPrerequisite>
	  <skillRequirements>
        <Crafting>8</Crafting>
      </skillRequirements>
    </recipeMaker>
	<techLevel>Industrial</techLevel>
    <graphicData>
      <texPath>Things/Apparel/GermUni/Germ</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
	<costList>
      <Steel>75</Steel>
	  <ComponentIndustrial>1</ComponentIndustrial>
