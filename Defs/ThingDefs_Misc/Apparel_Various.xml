	  <StuffEffectMultiplierInsulation_Heat>0.60</StuffEffectMultiplierInsulation_Heat>
	  <StuffEffectMultiplierArmor>5</StuffEffectMultiplierArmor>
      <Mass>3.5</Mass>
      <Bulk>5</Bulk>
	  <EquipDelay>5</EquipDelay>
