	</projectile>
  </ThingDef>

  <ThingDef Class="CombatExtended.AmmoDef" ParentName="Base25x163mmBullet">
    <defName>Bullet_25x163mm_HE</defName>
    <label>25x163mm bullet (HE)</label>
