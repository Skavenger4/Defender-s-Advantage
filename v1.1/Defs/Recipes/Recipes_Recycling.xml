<Defs>

	<!--=====================Musket Ball Recycling===================-->
	
	<!--=====================Carbon Rich Fuel Liquification===================-->
	<RecipeDef>
      <defName>Make_LiquifiedFuel</defName>
      <label>Make 35 Chemfuel through Coal Liquification</label>
      <description>Make 35 Chemfuel through Coal Liquification.</description>
      <jobString>Making 35 Chemfuel through Coal Liquification</jobString>  
	  <allowMixingIngredients>true</allowMixingIngredients>
      <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Charcoal</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Charcoal</li>
      </thingDefs>
    </fixedIngredientFilter>
      <products>
         <Chemfuel>35</Chemfuel>
      </products>
      <workAmount>4500</workAmount>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Intellectual</workSkill>
      <effectWorking>Tailor</effectWorking>
      <soundWorking>Recipe_Tailor</soundWorking>
	  <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
      <recipeUsers>
        <li>BiofuelRefinery</li>
      </recipeUsers>
	</RecipeDef>
